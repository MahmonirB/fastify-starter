{
    "extends": "fastify-tsconfig",
    "compilerOptions": {
    "outDir": "dist",
    "declaration": false,
    "sourceMap": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/controller": [
        "./src/controller/*"
      ]
    }
    },
    "include": ["./src/**/*.ts", "./src/index.ts"],
}