{
  "compilerOptions": {
    "outDir": "dist",
    "declaration": false,
    "sourceMap": true,
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/controller": [
        "./src/controller/*"
      ]
    }
  },
  "include": [
    "./src/**/*.ts"
  ],
  // "exclude": ["dist", "node_modules"]
}